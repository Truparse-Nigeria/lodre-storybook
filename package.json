{
  "name": "truparse-lodre",
  "version": "1.0.14",
  "private": false,
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/Truparse-Nigeria/lodre-storybook",
  "repository": {
    "type": "git",
    "url": "https://github.com/Truparse-Nigeria/lodre-storybook.git"
  },
  "bugs": {
    "url": "https://github.com/Truparse-Nigeria/lodre-storybook/issues",
    "email": "workstation@truparse.com"
  },
  "dependencies": {
    "styled-components": "^5.3.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "tsc",
    "test": "react-scripts test",
    "compile-icons": "npx @svgr/cli --icon -d src/icons src/icons --typescript",
    "copy-fonts": "copyfiles -u 1 src/config/fonts/* lib/",
    "copy-styles": "copyfiles -u 1 src/config/styles/* lib/",
    "eject": "react-scripts eject",
    "postbuild": "yarn copy-fonts && yarn copy-styles",
    "prebuild": "rm -rf ./lib && yarn compile-icons",
    "prepublish": "yarn build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-docs": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/node-logger": "^6.4.9",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.9",
    "@svgr/cli": "^6.1.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.18",
    "copyfiles": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  }
}
